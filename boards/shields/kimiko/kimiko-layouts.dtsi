#include <physical_layouts.dtsi>

/ {
    default_layout: default_layout {
        compatible = "zmk,physical-layout";
        display-name = "Kimiko Default";
        transform = <&default_transform>;
        kscan = <&kscan>;

        keys
        =
        // Left half
        <&key_physical_attrs 100 100  0    0     0 0 0>,  // 0,0
        <&key_physical_attrs 100 100  100  0     0 0 0>,  // 0,1
        <&key_physical_attrs 100 100  200  0     0 0 0>,  // 0,2
        <&key_physical_attrs 100 100  300  0     0 0 0>,  // 0,3
        <&key_physical_attrs 100 100  400  0     0 0 0>,  // 0,4
        <&key_physical_attrs 100 100  500  0     0 0 0>,  // 0,5

        <&key_physical_attrs 100 100  0    100   0 0 0>,  // 1,0
        <&key_physical_attrs 100 100  100  100   0 0 0>,  // 1,1
        <&key_physical_attrs 100 100  200  100   0 0 0>,  // 1,2
        <&key_physical_attrs 100 100  300  100   0 0 0>,  // 1,3
        <&key_physical_attrs 100 100  400  100   0 0 0>,  // 1,4
        <&key_physical_attrs 100 100  500  100   0 0 0>,  // 1,5

        <&key_physical_attrs 100 100  0    200   0 0 0>,  // 2,0
        <&key_physical_attrs 100 100  100  200   0 0 0>,  // 2,1
        <&key_physical_attrs 100 100  200  200   0 0 0>,  // 2,2
        <&key_physical_attrs 100 100  300  200   0 0 0>,  // 2,3
        <&key_physical_attrs 100 100  400  200   0 0 0>,  // 2,4
        <&key_physical_attrs 100 100  500  200   0 0 0>,  // 2,5

        <&key_physical_attrs 100 100  0    300   0 0 0>,  // 3,0
        <&key_physical_attrs 100 100  100  300   0 0 0>,  // 3,1
        <&key_physical_attrs 100 100  200  300   0 0 0>,  // 3,2
        <&key_physical_attrs 100 100  300  300   0 0 0>,  // 3,3
        <&key_physical_attrs 100 100  400  300   0 0 0>,  // 3,4
        <&key_physical_attrs 100 100  500  300   0 0 0>,  // 3,5

        <&key_physical_attrs 100 100  200  400   0 0 0>,  // 4,0
        <&key_physical_attrs 100 100  300  400   0 0 0>,  // 4,1
        <&key_physical_attrs 100 100  400  400   0 0 0>,  // 4,2
        <&key_physical_attrs 100 100  500  400   0 0 0>,  // 4,3
        <&key_physical_attrs 100 150  600  425  30 650 425>, // 4,4 thumb

        // Right half (col-offset=6 applied automatically)
        <&key_physical_attrs 100 100  800   0     0 0 0>,  // 5,5
        <&key_physical_attrs 100 100  900   0     0 0 0>,  // 5,4
        <&key_physical_attrs 100 100  1000  0     0 0 0>,  // 5,3
        <&key_physical_attrs 100 100  1100  0     0 0 0>,  // 5,2
        <&key_physical_attrs 100 100  1200  0     0 0 0>,  // 5,1
        <&key_physical_attrs 100 100  1300  0     0 0 0>,  // 5,0

        <&key_physical_attrs 100 100  800   100   0 0 0>,  // 6,5
        <&key_physical_attrs 100 100  900   100   0 0 0>,  // 6,4
        <&key_physical_attrs 100 100  1000  100   0 0 0>,  // 6,3
        <&key_physical_attrs 100 100  1100  100   0 0 0>,  // 6,2
        <&key_physical_attrs 100 100  1200  100   0 0 0>,  // 6,1
        <&key_physical_attrs 100 100  1300  100   0 0 0>;  // 6,0
    };
};
